#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "ru");
    int a, b, c, max;
    cout << "Введите 3 любых числа" << endl;
    cin >> a >> b >> c;
    if (a >= b) {
        max = a;
    }
    else {
        if (b >= c) {
            max = b;
        }
        else {
            max = c;
        }
    }
    cout << "Максимальное число " << max;
    return 0;
}